  <main>

<h1 class="text-center text-light">{{Leaguestuff['Name']}}</h1>
   
   <a class="btn btn-secondary btn-lg  "  href="/feed" role="button"><i class="bi bi-arrow-left-circle-fill"></i> </a>   
 <div class="text-center">
    {% for League in Leagues %}
         <a href="/League/{{League.ID}}"> <img src="{{League.Logo}}" height="65"></a>
          {%endfor%}
          </div>
    <div class="album py-5 ">
      <div class="container">






        <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3">
        {% for id, sites in Games | groupby('ID') %}
          {% set game = sites[0] %}
          <div class="col">
            <div class="card shadow-sm">
            <div class="text-center">
             <img src={{game.Logo}} width="120" height="120"> VS <img src={{game["t2.Logo"]}} width="120" height="120">

            </div>
              <div class="card-body">
                <p class=" text-center card-text">{{ game["Team"] }} VS {{game["t2.Team"]}}</p>
                <div class="d-flex justify-content-between align-items-center">
                  <div class="btn-group">
                    <div class="dropup">
                      <button class="btn btn-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown"
                        aria-expanded="false">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor"
                          class="bi bi-tv" viewBox="0 0 16 16">
                          <path
                            d="M2.5 13.5A.5.5 0 0 1 3 13h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5M13.991 3l.024.001a1.5 1.5 0 0 1 .538.143.76.76 0 0 1 .302.254c.067.1.145.277.145.602v5.991l-.001.024a1.5 1.5 0 0 1-.143.538.76.76 0 0 1-.254.302c-.1.067-.277.145-.602.145H2.009l-.024-.001a1.5 1.5 0 0 1-.538-.143.76.76 0 0 1-.302-.254C1.078 10.502 1 10.325 1 10V4.009l.001-.024a1.5 1.5 0 0 1 .143-.538.76.76 0 0 1 .254-.302C1.498 3.078 1.675 3 2 3zM14 2H2C0 2 0 4 0 4v6c0 2 2 2 2 2h12c2 0 2-2 2-2V4c0-2-2-2-2-2" />
                        </svg>
                      </button>
                      <ul class="dropdown-menu">
                        {% for Site in sites %}
                        <li><a class="dropdown-item" href="{{Site["URL"]}}"><img src="{{Site["Sites.Logo"]}}" width="50"></a> </li>
                      
                        {%endfor%}
                      </ul>
                    </div>
                  </div>
                  <small class="text-body-secondary">{{game['Datetime'].strftime('%B %d %Y %I:%M %p') }}</small>
                </div>
              </div>
            </div>
          </div>
          {%endfor%}
        </div>
      </div>
    </div>



  </main>





   
      
      
          

  



